@model PollModel
@using Nop.Web.Models.Polls;
<div class="block block-poll">
    <div class="title">
        <strong>@T("Polls.Title")</strong>
    </div>
    <div class="listbox">
        @Html.Partial("_Poll", Model)
    </div>
</div>
<nav class="navbar navbar-default panel panel-default" role="navigation">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#poll-sidebar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand visible-xs" href="#">@T("Polls.Title")</a>
    </div>
    <div class="panel-heading hidden-xs">
        <h3 class="panel-title">@T("Polls.Title")</h3>
    </div>
    <div class="navbar-collapse collapse" id="poll-sidebar">
        <div class="panel-body">
            <div class="listbox">
                @Html.Partial("_Poll", Model)
            </div>
        </div>
    </div>
</nav>

